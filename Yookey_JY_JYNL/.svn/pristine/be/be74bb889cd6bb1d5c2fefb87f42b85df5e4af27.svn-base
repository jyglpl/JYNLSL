.menuList{float:left; clear:both; height:100%; width:99%;margin: 0 auto; margin-top:5px;margin-bottom: 5px;border:solid 1px #85b6df;} 
.menuList .menu_tit{ width:100%;  height:32px; width:100%; float:left; clear:both;background: url("../Images/block_top_bg.png") no-repeat scroll 0 -32px transparent;background-position: 0 -32px;background-repeat: repeat-x; border-bottom:solid 1px #85b6df;}
.menuList .menu_tit h3{ float: left;font-size: 12px;font-weight: bold;line-height: 16px; margin-left: 13px; padding:11px 0;color: #393939; }
.menuList .menu_tit h3 input{ margin-right:3px; margin-top:-2px;}
.menuList .menu_content{padding-top:10px; float:left; clear:both; width:100%;}

