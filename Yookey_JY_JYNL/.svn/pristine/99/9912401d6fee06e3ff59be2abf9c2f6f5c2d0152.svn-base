<script type="text/javascript">
    function OpenPunishCase(type) {
        var url = '';
        if (type == '00950002') {    //国土
            url = '@Url.Action("Index", "PenaltyCaseLand")';
        }
        else if (type == '00950003') {  //安监
            url = '@Url.Action("CaseSaftyAnalysisIndex", "PenaltyCaseSafty")';
        }
        else if (type == '00950004') {  //渔政
            url = '@Url.Action("CaseSeaIndex", "PenaltyCaseSea")';
        }
        else if (type == '00950005') {  //水利
            url = '@Url.Action("Index", "PenaltyCaseWater")?type=' + type;
        }
        else {
            url = '@Url.Action("Index", "PenaltyCase")?type=' + type;
        }
    location.href = url;
}

</script>

