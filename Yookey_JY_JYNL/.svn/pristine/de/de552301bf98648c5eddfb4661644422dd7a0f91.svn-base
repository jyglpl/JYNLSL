.loginTop {
    width: 100%;
    height: 45%;
    background: url(../img/loginTop.png) no-repeat;
    background-size: 100% 100%;
    margin: 0px;
}

.loginBottom {
    width: 100%;
    height: 10%;
    background: url(../img/loginBottom.png) no-repeat;
    background-size: 100% 100%;
    margin: 0px;
}

.loginBetween {
    width: 100%;
    height: 45%;
    background: url(../img/loginBteweent.png) no-repeat;
    background-size: 100% 100%;
    border: 1px solid transparent;
}

    .loginBetween > div {
        width: 400px;
        margin: 50px auto;
        overflow: hidden;
    }

        .loginBetween > div p {
            width: 100%;
            height: 56px;
            margin-bottom: 15px;
        }

            .loginBetween > div p b {
                width: 56px;
                height: 56px;
            }

            .loginBetween > div p span {
                width: 343px;
                height: 56px;
            }

                .loginBetween > div p span input {
                    width: 343px;
                    height: 56px;
                    border: 1px solid #c8d0d5;
                    padding-left: 15px;
                    font-size: 16px;
                }

            .loginBetween > div p .nameIcon {
                background: url(../img/nameIcon.png) no-repeat;
                background-size: 100% 100%;
            }

            .loginBetween > div p .passwordIcon {
                background: url(../img/passwordIcon.png) no-repeat;
                background-size: 100% 100%;
            }

            .loginBetween > div p > input {
                width: 180px;
                height: 56px;
                color: #fff;
                font-size: 18px;
                border: none;
            }

            .loginBetween > div p .logBtn {
                margin-right: 35px;
                background-color: #0076c0;
            }

            .loginBetween > div p .logRem {
                background-color: #00a0e9;
            }

            .loginBetween > div p:last-child {
                margin-top: 50px;
            }

