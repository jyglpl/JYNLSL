header {
    width: 100%;
    height: 84px;
    background: url(../img/topBackground.png) no-repeat;
    background-size: 100% 100%;
    overflow: hidden;
}

    header .logoLeft {
        margin: 17px 11px;
    }

        header .logoLeft span {
            font-size: 32px;
            color: #fff;
            vertical-align: sub;
        }

    header div {
        margin: 27px 18px 0px 0px;
    }

        header div ul {
            overflow: hidden;
        }

            header div ul li {
                float: left;
                margin-left: 30px;
            }

                header div ul li a {
                    color: #fff;
                    padding: 5px 2px;
                    display: block;
                }

                    header div ul li a span {
                        font-size: 14px;
                        vertical-align: super;
                    }

                    header div ul li a b.Ahome {
                        display: inline-block;
                        width: 20px;
                        height: 20px;
                        background: url(../img/homeIcon.png) no-repeat;
                        background-size: 100% 100%;
                    }

                    header div ul li a b.Amessage {
                        display: inline-block;
                        width: 20px;
                        height: 20px;
                        background: url(../img/messageIcon.png) no-repeat;
                        background-size: 100% 100%;
                    }

                    header div ul li a b.Aexit {
                        display: inline-block;
                        width: 20px;
                        height: 20px;
                        background: url(../img/exitIcon.png) no-repeat;
                        background-size: 100% 100%;
                    }

.contentBox {
    padding: 0px;
    background-color: #e5e5e5;
    width: 100%;
    overflow: hidden;
    min-height: 85%;
    position: relative;
}

    .contentBox .secondLi {
        height: 50px;
    }

        .contentBox .secondLi ul {
            overflow: hidden;
        }

            .contentBox .secondLi ul li {
                float: left;
                margin-right: 30px;
                line-height: 42px;
            }

                .contentBox .secondLi ul li a {
                    font-size: 18px;
                    color: #212529;
                    font-family: initial;
                }

                .contentBox .secondLi ul li .SecondColor {
                    color: #ff5353;
                }

    .contentBox .searchBox {
        width: 350px;
        position: relative;
        margin-top: 7px;
    }

        .contentBox .searchBox span {
            float: left;
        }

            .contentBox .searchBox span:first-child {
                position: absolute;
                left: 0px;
            }

            .contentBox .searchBox span:nth-child(2) {
                width: 290px;
            }

                .contentBox .searchBox span:nth-child(2) input {
                    width: 100%;
                    padding-left: 110px;
                }

    .contentBox .contentCase {
        width: 100%;
        margin-bottom: 30px;
        overflow: hidden;
    }

        .contentBox .contentCase .liTopParent {
            width: 100%;
        }

            .contentBox .contentCase .liTopParent > p {
                width: 100%;
                height: 45px;
                background-color: #f2f6fc;
                line-height: 45px;
                border: 1px solid #ced8e4;
            }

                .contentBox .contentCase .liTopParent > p span {
                    margin-left: 21px;
                    color: #ffb400;
                    font-size: 18px;
                }

            .contentBox .contentCase .liTopParent div {
                width: 99.5%;
                background-color: #f9f9f9;
                border-left: 1px solid #ced8e4;
                padding-left: 18%;
                padding-top: 20px;
                height: 652px;
            }

                .contentBox .contentCase .liTopParent div ul li {
                    width: 100%;
                    margin-bottom: 20px;
                }

                    .contentBox .contentCase .liTopParent div ul li a {
                        color: #8f99ac;
                        font-size: 16px;
                    }

                        .contentBox .contentCase .liTopParent div ul li a:hover {
                            color: #666;
                        }

                        .contentBox .contentCase .liTopParent div ul li a:active {
                            color: #ff5353;
                        }

                    .contentBox .contentCase .liTopParent div ul li .colorChoose {
                        color: #ff5353;
                    }

                        .contentBox .contentCase .liTopParent div ul li .colorChoose:hover {
                            color: #ff5353;
                        }

        .contentBox .contentCase .contenForm .stynse {
            width: 100%;
            margin-top: 30px;
            text-align: center;
        }

            .contentBox .contentCase .contenForm .stynse b {
                font-weight: normal;
                color: #0567d3;
                font-size: 18px;
                margin-right: 10px;
                cursor: pointer;
            }

        .contentBox .contentCase .contenForm > .titleChart {
            width: 100%;
            height: 45px;
            background-color: #f2f6fc;
            line-height: 45px;
            border: 1px solid #ced8e4;
            overflow: hidden;
            padding-right: 20px;
        }

            .contentBox .contentCase .contenForm > .titleChart b {
                color: #0567d3;
                font-size: 18px;
                margin-right: 10px;
                font-weight: normal;
                cursor: pointer;
            }

            .contentBox .contentCase .contenForm > .titleChart span {
                margin-left: 21px;
                color: #212429;
                font-size: 18px;
            }

            .contentBox .contentCase .contenForm > .titleChart div p {
                float: left;
                width: 387px;
            }

                .contentBox .contentCase .contenForm > .titleChart div p span {
                    float: left;
                    font-size: 14px;
                    color: #8f99ac;
                }

                    .contentBox .contentCase .contenForm > .titleChart div p span input {
                        width: 130px;
                        border: none;
                        background-color: #fff;
                        height: 20px;
                        font-size: 14px;
                        margin-top: 12.5px;
                    }

        .contentBox .contentCase .contenForm > p {
            width: 100%;
            height: 45px;
            background-color: #f2f6fc;
            line-height: 45px;
            border: 1px solid #ced8e4;
            overflow: hidden;
            padding-right: 20px;
        }

            .contentBox .contentCase .contenForm > p b {
                color: #0567d3;
                font-size: 18px;
                margin-right: 10px;
                font-weight: normal;
                cursor: pointer;
            }

            .contentBox .contentCase .contenForm > p span {
                margin-left: 21px;
                color: #212429;
                font-size: 18px;
            }

        .contentBox .contentCase .contenForm > div {
  
            overflow: hidden;
            background-color: #fff;
        }

            .contentBox .contentCase .contenForm > div > p {
                width: 100%;
                border-bottom: 1px solid #ced8e4;
                height: 55px;
                padding-left: 20px;
                padding-top: 18.5px;
                overflow: hidden;
            }

            .contentBox .contentCase .contenForm > div form {
                margin: 10px;
            }

        .contentBox .contentCase .contenForm .classificationBox {
            width: 100%;
            background-color: #f2f6fc;
            height: 45px;
        }

            .contentBox .contentCase .contenForm .classificationBox p:first-child {
                width: 20%;
                border-right: 1px solid #bfbfbf;
                height: 45px;
                border-bottom: none;
                padding-top: 0px;
                float: left;
                line-height: 45px;
            }

                .contentBox .contentCase .contenForm .classificationBox p:first-child span {
                    color: #ffb400;
                    font-size: 16px;
                }

            .contentBox .contentCase .contenForm .classificationBox p:last-child {
                float: left;
                width: 75%;
                padding-top: 0px;
                height: 45px;
                line-height: 45px;
                border-bottom: none;
            }

                .contentBox .contentCase .contenForm .classificationBox p:last-child span {
                    font-size: 16px;
                    margin-right: 15px;
                    cursor: pointer;
                }

        .contentBox .contentCase .contenForm .contenTable {
            padding: 15px;
        }

            .contentBox .contentCase .contenForm .contenTable table thead {
                font-size: 16px;
            }

                .contentBox .contentCase .contenForm .contenTable table thead th {
                    text-align: center;
                }

                    .contentBox .contentCase .contenForm .contenTable table thead th:nth-child(1) {
                        width: 5%;
                    }

                    .contentBox .contentCase .contenForm .contenTable table thead th:nth-child(2) {
                        width: 10%;
                    }

                    .contentBox .contentCase .contenForm .contenTable table thead th:nth-child(3) {
                        width: 8%;
                    }

                    .contentBox .contentCase .contenForm .contenTable table thead th:nth-child(4) {
                        width: 15%;
                    }

                    .contentBox .contentCase .contenForm .contenTable table thead th:nth-child(5) {
                        width: 5%;
                    }

                    .contentBox .contentCase .contenForm .contenTable table thead th:nth-child(6) {
                        width: 8%;
                    }

                    .contentBox .contentCase .contenForm .contenTable table thead th:nth-child(7) {
                        width: 9%;
                    }

                    .contentBox .contentCase .contenForm .contenTable table thead th:nth-child(8) {
                        width: 8%;
                    }

                    .contentBox .contentCase .contenForm .contenTable table thead th:nth-child(9) {
                        width: 8%;
                    }

                    .contentBox .contentCase .contenForm .contenTable table thead th:nth-child(10) {
                        width: 8%;
                    }

                    .contentBox .contentCase .contenForm .contenTable table thead th:nth-child(11) {
                        width: 8%;
                    }

            .contentBox .contentCase .contenForm .contenTable table tbody {
                font-size: 14px;
                text-align: center;
            }

                .contentBox .contentCase .contenForm .contenTable table tbody tr td a {
                    white-space: nowrap;
                }

                    .contentBox .contentCase .contenForm .contenTable table tbody tr td a:hover {
                        color: inherit;
                    }

                .contentBox .contentCase .contenForm .contenTable table tbody tr .asOf {
                    background-color: #ec6941;
                    color: #fff;
                }

                .contentBox .contentCase .contenForm .contenTable table tbody tr .Special {
                    color: #ffb400;
                }

                .contentBox .contentCase .contenForm .contenTable table tbody tr .Warning {
                    color: #FF5252;
                }

                .contentBox .contentCase .contenForm .contenTable table tbody tr .Secondary {
                    color: #17c671;
                }

        .contentBox .contentCase .contenForm .caseDetail fieldset {
            border: 1px solid silver;
            padding: .35em .625em .75em;
            margin-top: 20px;
        }

            .contentBox .contentCase .contenForm .caseDetail fieldset legend {
                width: inherit;
                border: none;
                padding: 0px 10px;
            }

            .contentBox .contentCase .contenForm .caseDetail fieldset .DSROne li {
                width: 100%;
                overflow: hidden;
            }

                .contentBox .contentCase .contenForm .caseDetail fieldset .DSROne li b {
                    float: left;
                    width: 30%;
                    text-align: right;
                }

                .contentBox .contentCase .contenForm .caseDetail fieldset .DSROne li span {
                    width: 60%;
                    float: left;
                    margin-left: 3%;
                }

                .contentBox .contentCase .contenForm .caseDetail fieldset .DSROne li a:hover {
                    color: #337ab7;
                }

            .contentBox .contentCase .contenForm .caseDetail fieldset ul {
                margin-left: 10px;
            }

                .contentBox .contentCase .contenForm .caseDetail fieldset ul li {
                    margin-bottom: 8px;
                }

                    .contentBox .contentCase .contenForm .caseDetail fieldset ul li b {
                        margin-right: 5px;
                        color: #576274;
                    }

                    .contentBox .contentCase .contenForm .caseDetail fieldset ul li span {
                        color: #353d4a;
                    }

                    .contentBox .contentCase .contenForm .caseDetail fieldset ul li a:hover {
                        color: #337ab7;
                    }
