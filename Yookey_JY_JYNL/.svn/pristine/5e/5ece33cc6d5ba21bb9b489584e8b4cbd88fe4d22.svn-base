    <!--固定元素-->
    <script src="~/Librarys/stickUp/stickUp.min.js"></script>
    <!--固定元素end-->

    <style type="text/css">
        .radioLeft {
            margin-left: 10px;
        }

        .formTitle {
            width: 145px;
        }
    </style>
</head>
<body>
    <div>
