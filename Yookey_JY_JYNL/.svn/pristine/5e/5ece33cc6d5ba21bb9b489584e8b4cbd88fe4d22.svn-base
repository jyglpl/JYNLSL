                            @Html.TextBoxFor(x => x.SetUpEndDate, "", new { @class = "txtborder", @style = "width: 150px;", @onfocus = "WdatePicker({dateFmt:'yyyy-MM-dd'})", Value = Model.SetUpEndDate.Year < 2000 ? "" : Model.SetUpEndDate.ToString(AppConst.DateFormat) })

