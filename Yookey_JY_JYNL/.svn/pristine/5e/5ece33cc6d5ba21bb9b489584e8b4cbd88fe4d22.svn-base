                    var ischecked = entity.Id.Equals(OutDoorEntity.OutdoorType) ? "checked=checked" : "";
                                <span class="item">
                                    <input type="radio" name="OutdoorType" class="radioLeft" @ischecked id="@entity.Id" value="@entity.Id" />
                                    <label id="lab_@entity.Id" for="@entity.Id">@entity.RsKey</label>
                                </span>
                }
            }
                            }
