                            <label for="rdocsbjY">是</label>
                            <input type="radio" name="IsTest" id="rdocsbjN" class="radioLeft"  @(Model.IsTest == 0 ? "checked=checked" : "") value="0" />
                            <label for="rdocsbjN">否</label>
                        </td>
                    </tr>
                    <tr>
                        <td class="formTitle">备注：</td>
                        <td class="formValue" colspan="3">
