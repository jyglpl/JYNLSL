.dataBox{
    width: 96%;
    margin: 20px auto;
    .detuct{
        width: 100%;
        background-color: #fff;
        padding: 10px;
        .chooseBox{
            width: 100%;
            overflow: hidden;
           &>span{
                float: left;
                font-size: 18px;
                color: #666;
                margin-right: 15px;
                font-weight: bold;
            }
            p{
                float: left;
                span{
                    float: left;
                    margin-right: 5px;
                    select{
                        width: 150px;
                        height: 30px;
                        border-radius: 5px;
                        border: 1px solid #ddd;
                        color: #666;
                    }
                }
            }
        }
        .paintList{
            width: 100%;
            margin-top: 15px;
            ul{
                li{
                    display: inline-block;
                    width: 100px;
                    margin-right: 30px;
                    span{
                        display: block;
                        width: 100%;
                        text-align: center;
                    }
                    .scroe{
                        font-size: 18px;
                        color: #ec5a5a;
                        margin-bottom: 5px;
                        font-weight: bold;
                    }
                    .total{
                        color: #0482e0;

                    }
                    .expain{
                        font-size: 18px;
                        color: #666;
                    }
                }
            }
        }
    }
    .chartsBox{
        width: 100%;
        margin-top: 20px;
        overflow: hidden;
        .marksTop{
            float: left;
            width: 48%;
            background-color: #fff;
            height: 400px;
            padding: 10px;
            box-sizing: border-box;
            &>p{
                font-size: 27px;
                color: #4c4c4c;
                
            }
            div{
                width: 100%;
                height: 300px;

            }
        }
        .marksRight{
            float: left;
            margin-left: 2%;
            height: 400px;
            background-color: #fff;
            padding: 10px;
            box-sizing: border-box;
            width: 50%;
            .markChart{
                width: 50%;
                float: left;
                &>p{
                    font-size: 27px;
                    color: #4c4c4c;
                    
                }
                #scoreContent{
                    width: 100%;
                    height: 320px;
                }
            }
            .markList{
                float: left;
                width: 50%;
                margin-top: 10%;
                ul{
                    margin-left: 10px;
                    li{
                        margin-bottom: 30px;
                        width: 100%;
                        overflow: hidden;
                        b{
                            display: block;
                            float: left;
                            width: 18px;
                            height: 18px;
                            text-align: center;
                            color: #fff;
                            background-color: #ff4040;
                            font-size: 12px;
                            margin-right: 5px;
                            margin-top: 2px;
                           
                        }
                        &>span{
                            color: #666;
                            float: left;
                            width: 80%;
                          span{
                              color: #ff8c54;
                          }
                        }
                    }
                }
            }
        }
    }
    .tableBox{
        width: 100%;
        background-color: #fff;
        padding: 10px;
        box-sizing: border-box;
        margin-top: 20px;
        overflow: hidden;
        &>p{
            font-size: 27px;
            color: #4c4c4c;
            
        }
        div{
          
            width: 98%;
            margin: 0px auto;
            margin-top: 5px;
            table{
                width: 100%;
                thead{
                    tr{
                        width: 100%;
                        background-color: #f5f5f5;
                        height: 30px;
                        color: #818385;
                        td{
                            border: none;
                        }
                    }
                }
                tbody{
                    tr{
                        border-bottom: 1px solid #ddd;
                        height: 30px;
                        td{
                            text-align: center;
                            color: #666;
                            span{
                                display: block;
                                width: 30%;
                                margin: 0px auto;
                                height: 100%;
                                color: #fff;
                                background-color: #e65404
                            }
                            .green{
                                background-color: #09ae09;
                            }
                            a.downLoad{
                                color: #e65404;
                                margin-right: 10px;
                            }
                            a.look{
                                color: #3087e1;
                            }
                        }
                        .colorOgreng{
                            color: #e65404;
                        }
                    }
                }
            }
        }
    }
}
.inputBox{
    width: 100%;
    height: 44px;
    background-color: #fff;
    padding-left: 10px;
    box-sizing: border-box;
 
    p{
        float: left;
        margin-right: 25px;
        span{
            float: left;
            color: #666;
            font-size: 16px;
            margin-top: 9.5px;
            margin-right: 10px;
            select{
                width: 120px;
                height: 25px;
                border: 1px solid #ccc;
                border-radius: 5px;
                color: #888;
            }
            input{
                width: 150px;
                height: 25px;
                border: 1px solid #ccc;
               
                color: #888;
            }
        }
    }
    .keySearch{
        float: right;
        span:last-child{
            margin-left: -10px;
            button{
                width: 50px;
                height: 25px;
                background-color: #44a9f3;
                color: #fff;
                border: none;

            }
            input{
                padding-left: 10px;
            }
        }
    }

}
.tablePaging{
    width: 100%;
   overflow: hidden;
   margin-top: 20px;
   p{
       float: right;
       span{
           display: inline-block;
           border: 1px solid #44a9f3;
           padding: 1px 5px;
           color: #888;
           margin-left: 10px;
           cursor: pointer;
       }
       b{
           color: #888;
       }
   }
}