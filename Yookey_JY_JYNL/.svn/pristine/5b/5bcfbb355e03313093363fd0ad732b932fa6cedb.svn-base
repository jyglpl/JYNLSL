using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Yookey.WisdomClassed.SIP.Model.Mobile
{
    /// <summary>
    /// 手机-审批数据实体
    /// </summary>
    public class MobileApproval
    {
        /// <summary>
        /// 标题
        /// </summary>
        public string Title { get; set; }
        /// <summary>
        /// 时间
        /// </summary>
        public DateTime Time { get; set; }
        /// <summary>
        /// 表单编号
        /// </summary>
        public string FormID { get; set; }

        /// <summary>
        /// 待办事项编号
        /// </summary>
        public string WorklistId { set; get; }

        /// <summary>
        /// 流程编号
        /// </summary>
        public string ProcessID { get; set; }
        /// <summary>
        /// 页面处理地址
        /// </summary>
        public string ManageUrl { get; set; }
        /// <summary>
        /// 页面查看地址
        /// </summary>
        public string ViewUrl { set; get; }
        /// <summary>
        /// 数据类型
        /// </summary>
        public string Type { set; get; }
