<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!--执行的时间间隔，以毫秒计-->
    <add key="TimerInterval" value="1000"/>

    <!--是否执行下载文件1、代表是 0代表否-->
    <add key="Download" value="0"/>

    <!--是否需要往ftp上面上传文件1、代表是 0代表否-->
    <add key="isUpload" value="0"/>

    <!--下载ftp参数配置-->
    <add key="ftpAddress" value="ftp://192.168.1.94/"/>
    <add key="username" value="yookey"/>
    <add key="password" value="1"/>

    <!--下载ftp服务中所对应文件夹-->
    <add key="FTPPath" value="YJ\"/>
    <!--下载ftp服务中上传所对应文件夹 ，这个节点不要配置-->
    <add key="UpPath" value="YYJ\"/>
    <!--数据下载到本地所对应文件夹，需要提前创建好-->
    <add key="DownPath" value="D:\Down\"/>


    <!--上传ftp参数配置-->
    <add key="ftpAddressUp" value="192.168.1.94"/>
    <add key="usernameUp" value="yookey"/>
    <add key="passwordUp" value="1"/>

    <!--上传ftp服务器对应的文件夹,需要提前创建-->
    <add key="ftpUpPath" value="数据\"/>


    <add key="Days" value="3"/>
    <!--服务每天开始执行时间-->
    <add key="StartTime" value="153600"/>
    <!--日志路径，服务器上面提前要创建好-->
    <add key="ErrorAddress" value="E:\Application\SZCG\YookeySZCG\Yookey.FTPService\log\"/>
    <add key="ClientSettingsProvider.ServiceUri" value=""/>
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
