                                        <td>@Html.TextBoxFor(x => x.REPORT_PHONE, new { @class = "s_input", @readonly = "readonly" })</td>
                                    </tr>
                                    <tr>
                                        <th>问题来源：</th>
                                        <td>
                                            @Html.DropDownListFor(x => x.CASE_SOURCES, (ViewData["Sources"] as IEnumerable<SelectListItem>), new { @class = "s_select", @style = "width:250px;", disabled = "disabled" })
                                        </td>
                                    </tr>
                                    <tr>
                                        <th>上报时间：</th>
                                        <td>
                                            @Html.TextBoxFor(x => x.REPORT_CASE_DATE, new { @class = "s_input", @readonly = "readonly", Value = Convert.ToDateTime(Model.REPORT_CASE_DATE).ToString("yyyy-MM-dd") })
                                        </td>
                                    </tr>
                                    <tr>
                                        <th>所属部门</th>
                                        <td>
                                            @Html.TextBoxFor(x => x.ZF_DEPT_NAME, new { @class = "s_input", @readonly = "readonly" })
                                        </td>
                                    </tr>
                                    <tr>
                                        <th>承办人</th>
                                        <td>
                                            @Html.TextBoxFor(x => x.HANDLE_NAME, new { @class = "s_input", @readonly = "readonly" })
                                        </td>
                                    </tr>
                                    <tr>
                                        <th>问题描述：</th>
                                        <td>
                                            @Html.TextAreaFor(x => x.CASE_FACT, new { @class = "s_textarea", @style = "width:90%", @readonly = "readonly" })
                                        </td>
                                    </tr>
                                    <tr>
                                        <th>地址描述：</th>
                                        <td>
                                            @Html.TextAreaFor(x => x.DETAIL_ADDRESS, new { @class = "s_textarea", @style = "width:90%", @readonly = "readonly" })
                                        </td>
                                    </tr>
                                    @if (LeadStartIdea.Any())
                                    {
                                        <tr>
                                            <th>承办人意见：</th>
                                            <td>

                                                <textarea class="s_textarea" style="width: 90%">@(LeadStartIdea[0].Idea)</textarea>
                                                <div style="text-align: right; width: 90%;">
                                                    <span>签字：</span>
                                                    <span style="display: inline-block; width: 120px; border-bottom: 1px solid #ccc; text-align: left">@(LeadStartIdea[0].UserName)</span>
                                                    <span>日期：</span>
                                                    <span style="display: inline-block; width: 120px; border-bottom: 1px solid #ccc; text-align: left">@(Convert.ToDateTime(LeadStartIdea[0].CreateOn).ToString("yyyy-MM-dd"))</span>
                                                </div>

                                            </td>
                                        </tr>
                                    }
                                    @if (LeadIdea.Any())
                                    {
                                        <tr>
                                            <th>中队意见：</th>
                                            <td>

                                                <textarea class="s_textarea" style="width: 55%">@(LeadIdea[0].Idea)</textarea>
                                                <div style="text-align: right; width: 55%;">
                                                    <span>签字：</span>
                                                    <span style="display: inline-block; width: 120px; border-bottom: 1px solid #ccc; text-align: left">@(LeadIdea[0].UserName)</span>
                                                    <span>日期：</span>
                                                    <span style="display: inline-block; width: 120px; border-bottom: 1px solid #ccc; text-align: left">@(Convert.ToDateTime(LeadIdea[0].CreateOn).ToString("yyyy-MM-dd"))</span>
                                                </div>

                                            </td>
                                        </tr>
                                    }

                                    <tr class="trsave">
                                        <th></th>
                                        <td class="s_sb_bar">
                                            @if (Convert.ToInt32(Model.CASE_STATUS) == 50 && ViewBag.btnApplay)
                                            {
                                                <button class="sipac_btn s_b_submit" onclick="ApplyForFiled('apply', 0, 0)" id="btnApply">申请结案</button>
                                            }
                                            @if (ViewBag.BtnAgree)
                                            {
                                                <button class="sipac_btn s_b_submit" onclick="ApplyForFiled('agree', 1, 0)">同 意</button>
                                            }
                                        </td>
                                    </tr>
                                </table>
                            </div>
                        </div>                
                        
                        
                        
                        
                        }
                    }
                </div>
            </div>
        </div>
    </div>
</body>
<script type="text/javascript">

    var id = "@ViewBag.Id";
    var title = ""

    //模板下载
    function upLoadMB(obj) {
        var url = "@AppConfig.FileViewLink";
        if (obj == "LASP") {
            url = url + "Template/违建立案审批表.doc";
            window.open(url);
        }
        else if (obj == "CLSPYJ") {
            url = url + "Template/违建处理审批表.doc";
            window.open(url);
        };
    };

    function handle(obj) {
        var url = "";
        if (obj == "Verify") {
            url = '@Url.Action("IllegalVerify", "IllegalConstruction")?Pk_Id=' + id;
            title = "核实认定";
        }
        else if (obj == "Planning") {
            url = '@Url.Action("IllegalPlanning", "IllegalConstruction")?Pk_Id=' + id;
            title = "规划认定";
        }
        else if (obj == "Remove") {
            url = '@Url.Action("IllegalRemove", "IllegalConstruction")?Pk_Id=' + id;
                title = "组织拆除";
            }
    var width = $(window).width();
    width = width - (width * 0.1);
    var height = $(window).height();
    top.$.dialog({
        id: "Dialog",
        lock: true,
        title: title,
        max: false,
        min: false,
        width: 1200,
        height: height,
        content: 'url:' + url
    });
}


//=================案件审批======================
//结案审批
var worklistid = '@ViewBag.WorkListId';
    function ApplyForFiled(htype, isloadOp, isFaster) {
        var formId = '@ViewBag.Id';
        var url = '@Url.Action("IllegalWorkFlow", "WorkFlow", new { Area = "" })?formId=' + formId + '&flowName=违建结案审批&eType=' + htype + '&worklistId=' + worklistid + '&isloadOp=' + isloadOp + '&isFaster=' + isFaster + '&hidden=false';
        url = encodeURI(url);
        var width = 500;
        var height = 300;
        top.$.dialog({
            id: "WorkFlow", width: width, height: height, lock: true, max: false, resize: false, extendDrag: true,
            title: "案件审批",
            content: 'url:' + url,
            button: [
                {
                    name: '确 定',
                    className: 'green',
                    callback: function () {
                        top.frames["WorkFlow"].AcceptClick('MainDialog');
                        return true;
                    }
                }, { name: '关 闭' }
            ]
        });
    }


    function nextLink() {
        var updata = "";
        var hidStatus = $("#hidStatus").val();
        var status = '@ViewBag.Status';
        if (status == "21") {
            var Lasp = $("#LASPuploadTime").val();
            if (Lasp == "") {
                tipDialog('请输入立案审批日期！', 2, 0);
                return;
            }
            updata = Lasp;
        }
        else if (status == "28") {
            var Cfjd = $("#CFJDSuploadTime").val();
            if (Cfjd == "") {
                tipDialog('请输入处罚决定日期！', 2, 0);
                return;
            }
            updata = Cfjd;
        }
        $.ajax({
            cache: true,
            type: "POST",
            url: "@Url.Action("UpdateNextIllegalCase", "IllegalConstruction")",
            data: { Pk_Id: '@ViewBag.Id', hidStatus: hidStatus, status: status, updata: updata },
            datatype: 'json',
            success: function (data) {
                if (data.rtState == 1) {
                    if (status == "35") {
                        //window.location.href = "@Url.Action("IllegalRemove", "IllegalConstruction")?Pk_Id=" + '@ViewBag.CaseInfoNo';
                        var api = frameElement.api, w = api.opener;
                        api.get('MainDialog').location.reload();
                    } else {
                        window.location.reload();
                    }
                }
            }
        });
    }


    $(".Illegaltrue").click(function () {
        $(this).attr('checked', 'checked');
        $(".Illegalfalse").removeAttr('checked');
        $("#btnplan").css("display", "block");
        $("#btnpunish").css("display", "none");
        $("#btncancle").css("display", "block");
    });
    $(".Illegalfalse").click(function () {
        $(this).attr('checked', 'checked');
        $(".Illegaltrue").removeAttr('checked');
        $("#btnplan").css("display", "none");
        $("#btnpunish").css("display", "block");
        $("#btncancle").css("display", "block");
    });


    function planData() {
        var txtPlanTime = $("#PLAN_CUTOFF_DATE").val();
        var txtPlanNo = $("#PLAN_FILE_NUMBER").val();
        var isPunish = "";
        if ($(".Illegaltrue").attr('checked') == "checked") {
            isPunish = 0;
        }
        else {
            isPunish = 1;
        }
        $.ajax({
            cache: true,
            type: "POST",
            url: "@Url.Action("SubmitPlanData", "IllegalConstruction")",
            data: { CaseId: '@ViewBag.Id', txtPlanTime: txtPlanTime, txtPlanNo: txtPlanNo, isPunish: isPunish },
            datatype: 'json',
            success: function (data) {
                if (data.rtState == 1) {
                    if (isPunish == "0") {
                        tipDialog("进入组织拆除！", 2, 2);
                        window.location.reload();
                    }
                    else {
                        tipDialog("进入行政处罚流程！", 2, 2);
                        window.location.reload();
                    }
                }
                else {
                    //tipDialog("规划认定成功！", 2, 1);
                }
            }
        });
    }
</script>
</html>



